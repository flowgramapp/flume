{
  "name": "flume",
  "version": "0.6.3",
  "description": "A node editor for React",
  "author": "chrisjpatty",
  "license": "MIT",
  "homepage": "https://flume.dev",
  "repository": "chrisjpatty/flume",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts test --env=jsdom && npx cypress run",
    "test:watch": "react-scripts test --env=jsdom",
    "copy-files": "copyfiles -u 1 src/**/*.css .tsc-out/",
    "build": "rimraf dist && tsc && npm run copy-files && rollup -c",
    "start": "rollup -c -w",
    "release": "npx np",
    "clean": "rimraf dist .tsc-out",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "prettier": "prettier --write ."
  },
  "np": {
    "testScript": "test"
  },
  "peerDependencies": {
    "prop-types": "^15.5.4",
    "react": "17.x",
    "react-dom": "17.x"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-url": "^6.1.0",
    "@svgr/rollup": "^6.2.0",
    "@testing-library/dom": "^7.20.0",
    "@testing-library/react": "^10.4.3",
    "@types/d3-shape": "^3.0.2",
    "@types/lodash": "^4.14.178",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-portal": "^4.0.4",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "acorn-jsx": "^5.3.2",
    "autoprefixer": "^10.4.2",
    "copyfiles": "^2.4.1",
    "cross-env": "^5.1.4",
    "cypress": "^4.9.0",
    "eslint": "^8.7.0",
    "eslint-config-react-app": "^7.0.0",
    "gh-pages": "^1.2.0",
    "jest-puppeteer": "^4.4.0",
    "np": "^7.2.0",
    "postcss-nested": "^4.2.1",
    "prettier": "^2.5.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "^1.1.4",
    "rimraf": "^3.0.2",
    "rollup": "^2.64.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "tailwindcss": "^3.0.15",
    "typescript": "^4.5.4",
    "typescript-plugin-css-modules": "^3.4.0",
    "wait-on": "^6.0.0"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@reach/auto-id": "^0.16.0",
    "d3-shape": "^3.1.0",
    "lodash": "^4.17.21",
    "nanoid": "^3.2.0",
    "react-portal": "^4.2.1"
  }
}
